        "@types/underscore": "^1.8.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "concurrently": "^4.1.2",
        "dts-generator": "^3.0.0",
        "prettier": "^1.16.3",
        "stylelint": "^11.1.1",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-order": "^3.1.1",
        "tslint": "^5.20.1",
        "typescript": "^3.2.4"
