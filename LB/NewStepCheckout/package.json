        "@babel/preset-typescript": "^7.7.2",
        "@babel/types": "^7.7.2",
        "@comandeer/babel-plugin-banner": "^4.1.0",
        "@types/html2canvas": "0.0.35",
        "@types/jquery": "1.10.27",
